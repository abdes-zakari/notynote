{
  "name": "electron-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.0",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "electron-is-dev": "^1.0.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "knex": "^0.16.3",
    "react": "^16.7.0",
    "react-animate-height": "^2.0.7",
    "react-css-transition": "^0.7.4",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-select": "^2.4.2",
    "react-textarea-autosize": "^7.1.0",
    "sqlite3": "^4.0.6",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dist": "build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "ele": "electron . ",
    "electron-dev": "concurrently \" npm run start\" \"wait-on http://localhost:3000 && electron .\"",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-react --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/notes.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Electron React Note\"",
    "package-linux": "electron-packager . electron-react --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "main.js",
  "devDependencies": {
    "electron": "^9.4.0",
    "electron-builder": "^20.38.5",
    "electron-rebuild": "^1.8.2",
    "react-masonry-css": "^1.0.12"
  },
  "build": {
    "extends": null,
    "appId": "com.test.abdes",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "public/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
